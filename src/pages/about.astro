          <!-- Specializations -->
          <div class="bg-emerald-50/50 border border-emerald-100 rounded-2xl p-6">
            <h3 class="text-xl font-serif font-bold text-emerald-950 mb-4">Clinical Focus</h3>
            <ul class="space-y-2 text-gray-700">
              <li class="flex items-start">
                <span class="w-2 h-2 bg-emerald-600 rounded-full mt-2 mr-3 flex-shrink-0"></span>
                Chronic Pain Management
              </li>
